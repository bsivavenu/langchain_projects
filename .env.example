# LangChain Projects Environment Variables
# Copy this file to .env and fill in with your actual API keys
# WARNING: Never commit .env file with actual keys!

# OpenAI API Configuration
OPENAI_API_KEY=sk-proj-your-openai-api-key-here

# Google AI Studio API Configuration
GOOGLE_API_KEY=your-google-api-key-here

# HuggingFace API Token
HUGGINGFACEHUB_API_TOKEN=hf_your-huggingface-token-here

# Pinecone Vector Database Configuration
PINECONE_API_KEY=pcsk_your-pinecone-api-key-here
PINECONE_ENVIRONMENT=us-east-1
PINECONE_INDEX_NAME=your-index-name
PINECONE_INDEX=your-pinecone-index

# Supabase Configuration (if using Supabase)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_API_KEY=your-supabase-api-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Add any other API keys or configuration variables below
# Example:
# LANGCHAIN_API_KEY=your-langchain-api-key
# DATABASE_URL=postgresql://user:password@localhost:5432/dbname
